{
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#e60832",
    "window.activeBorder": "#9ccc65",
    "window.inactiveBorder": "#82b1ff",
    "activityBar.activeBorder": "#07000f",
    "[codeSTACKr Theme]": {
      "activityBar.activeBackground": "#87B6E6",
      "tab.activeBackground": "#0af820",
      "tab.inactiveBackground": "#898cf8",
      "tab.activeForeground": "#000",
      "tab.inactiveForeground": "#000",
      "editor.background": "#02042a"
    },
    "[Andromeda]": {
      "editor.background": "#10073D"
    },
    "[Coder Coder Dark Theme]": {
      "editor.background": "#10073D"
    },
    "activityBar.activeBackground": "#ff0000"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  "editor.formatOnPaste": true,
  "eslint.format.enable": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "html.format.indentInnerHtml": true,
  "editor.minimap.enabled": false,
  "editor.fontFamily": "Fira Code",
  "files.trimTrailingWhitespace": true,
  "editor.fontSize": 17,
  "editor.tabSize": 2
}